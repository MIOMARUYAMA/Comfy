<main>
  <div class="container">
    <div class="row col-3">
      <h4>会員一覧</h4>
    </div>
    <div class="container">
      <table class="table table-hover table-inverse">
        <thead>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @end_users.each do |end_user| %>
          <tr>
            <td><%= end_user.id %></td>
            <td><%= link_to end_user.user_name, admin_end_user_path(end_user.id) %></td>
            <td><%= end_user.is_active ? '有効' : '退会' %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
        <div class="row justify-content-center">
          <%= paginate @end_users %>
        </div>
    </div>
  </dic>
</main>

